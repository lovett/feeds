    "grunt-contrib-clean": "0.7.0",
    "grunt-contrib-concat": "0.5.1",
    "grunt-contrib-copy": "0.8.2",
    "grunt-contrib-less": "1.1.0",
    "grunt-contrib-uglify": "0.11.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-http": "2.0.0",
    "grunt-shell": "1.1.2",
    "grunt-string-replace": "1.2.1",
