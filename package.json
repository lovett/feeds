{
  "name": "headlines",
  "version": "0.0.0",
  "private": true,
  "description": "Feed reading via Angular, Node, and Redis",
  "repository": {
    "type": "git",
    "url": "http://github.com/lovett/headlines.git"
  },
  "main": "server/index.js",
  "scripts": {
    "server": "nodemon --watch server server/index.js",
    "scheduler": "nodemon --watch scheduler scheduler/index.js",
    "feedfetcher": "nodemon --watch feedfetcher feedfetcher/index.js",
    "lint": "eslint dispatcher test Gruntfile.js",
    "hooks": "cp hooks/* .git/hooks/",
    "test": "mocha --bail --reporter min test/dispatcher",
    "coverage": "istanbul cover --dir coverage/dispatcher _mocha -- -R min test/dispatcher"
  },
