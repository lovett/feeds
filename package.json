    "grunt-contrib-copy": "0.8.1",
    "grunt-contrib-less": "1.0.1",
    "grunt-contrib-uglify": "0.9.2",
    "grunt-contrib-watch": "0.6.1",
    "grunt-http": "1.6.0",
    "grunt-shell": "1.1.2",
    "grunt-string-replace": "1.2.0",
