    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-concat": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-less": "1.2.0",
    "grunt-contrib-uglify": "1.0.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-http": "2.0.1",
    "grunt-shell": "1.2.1",
    "grunt-string-replace": "1.2.1",
    "istanbul": "0.4.2",
