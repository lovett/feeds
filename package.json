{
  "name": "headlines",
  "version": "0.0.0",
  "private": true,
  "description": "Feed reading via Angular, Node, and Redis",
  "repository": {
    "type": "git",
    "url": "http://github.com/lovett/headlines.git"
  },
  "main": "server/index.js",
  "scripts": {
    "postinstall": "npm prune",
    "server": "nodemon --watch server server/index.js",
    "scheduler": "nodemon --watch scheduler scheduler/index.js",
    "feedfetcher": "nodemon --watch feedfetcher feedfetcher/index.js",
    "lint": "eslint dispatcher test Gruntfile.js",
    "hooks": "cp hooks/* .git/hooks/",
    "test": "mocha --bail --reporter min test/dispatcher",
    "coverage": "istanbul cover --dir coverage/dispatcher _mocha -- -R min test/dispatcher"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "3.12.2",
    "eslint-plugin-mocha": "4.8.0",
    "grunt": "1.0.1",
    "grunt-angular-templates": "1.1.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-less": "1.4.0",
    "grunt-contrib-uglify": "2.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-http": "2.2.0",
    "grunt-shell": "2.1.0",
    "grunt-string-replace": "1.3.1",
    "istanbul": "0.4.5",
    "load-grunt-tasks": "3.5.2",
    "mocha": "3.2.0",
    "mockfirebase": "0.12.0",
    "nock": "9.0.2",
    "sinon": "1.17.7"
  },
  "dependencies": {
    "angular": "1.6.1",
    "angular-resource": "1.6.1",
    "angular-route": "1.6.1",
    "bunyan": "1.8.5",
    "entities": "1.1.1",
    "firebase": "3.6.4",
    "grunt-autoprefixer": "3.0.4",
    "grunt-contrib-cssmin": "1.0.2",
    "htmlparser": "1.7.7",
    "htmlparser2": "3.9.2",
    "jquery": "3.1.1",
    "moment": "2.17.1",
    "needle": "1.4.3",
    "normalize.css": "5.0.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "redis": "2.6.3",
    "request": "2.79.0",
    "restify": "4.3.0",
    "sqlite3": "3.1.8",
    "string": "3.3.3",
    "xml2js": "0.4.17",
    "xmlbuilder": "8.2.2"
  }
}
